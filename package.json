{
    "name": "poorinter",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@formatjs/intl-relativetimeformat": "11.2.6",
        "@material-ui/core": "4.x",
        "@material-ui/icons": "4.x",
        "@material-ui/pickers": "3.3.11",
        "@material-ui/styles": "4.x",
        "firebase": "8.x",
        "intl": "1.x",
        "intl-locales-supported": "1.x",
        "intl-pluralrules": "1.x",
        "react": "16.x",
        "react-dom": "16.x",
        "react-helmet": "5.x",
        "react-intl": "3.x",
        "react-loadable": "5.x",
        "react-router": "5.x",
        "react-router-dom": "5.x",
        "react-scripts": "5.x",
        "redux": "4.2.1",
        "redux-form": "8.x",
        "redux-form-material-ui": "5.0.0-beta.3",
        "redux-logger": "3.x",
        "redux-persist": "6.x",
        "redux-thunk": "2.x",
        "rmw-shell": "7.x",
        "source-map-explorer": "2.x"
    },
    "devDependencies": {
        "@types/node": "^12.12.7",
        "@types/react": "^16.9.11",
        "@types/react-dom": "^16.9.4",
        "@types/react-helmet": "^5.0.14",
        "@types/react-router-dom": "^5.1.2",
        "typescript": "~4.3.0"
    },
    "scripts": {
        "check": "npm-check -s -u",
        "analyze": "source-map-explorer build/static/js/main.*",
        "start": "PORT=3002 react-scripts start",
        "standard": "standard src/**/*.js",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "deploy": "yarn build && firebase use poop-rinter && firebase deploy --only hosting"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
